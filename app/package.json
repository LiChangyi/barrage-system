{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    
  },
  "scripts": {
    "start": "npm run dev",
    "webpackMain": "webpack --config webpack/webpack.main.js -w",
    "dev": "cross-env BROWSER=none NODE_ENV=development concurrently \"npm run devRenderer\" \"wait-on http://localhost:3000 && npm run devMain\"",
    "devRenderer": "react-scripts start",
    "devMain": "concurrently \"npm run webpackMain\" \"nodemon --watch ./build/main.js --exec 'electron ./build/main.js'\"",
    "buildRenderer": "react-scripts build",
    "buildMain": "webpack --config webpack/webpack.main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.3",
    "nodemon": "^2.0.1",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  }
}
